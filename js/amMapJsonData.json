{
    "mapchartdata": {
        "legend": {},
        "geodata": "worldLow",
        "projection": "Miller",
        "series": [
            {
                "dataSource" : {
                    "url" : "../js/amMapChartData.json"
                },
                "exclude" : ["AQ"],
                "type": "MapPolygonSeries",
                "useGeodata": true,
                "mapPolygons": {
                    "template": {
                        "tooltipText": "{name}",
                        "fill": "#12769E",
                        "nonScallingStroke": true,
                        "strokeOpacity": ".02",
                        "propertyFields" : {
                            "fill" : "fill"
                        },
                        "states": {
                            "hover": {
                                "properties": {
                                    "fill": "#293B56"
                                }
                            }
                        }
                    }
                }
            }
        ]
    }
}